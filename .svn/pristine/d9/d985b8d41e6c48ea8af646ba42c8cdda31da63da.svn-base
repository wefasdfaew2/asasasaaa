.game-contain-j {
  display: table;
  table-layout: fixed;
  float: left;
  width: 100%;
  height: 100%;
  padding: 5px 5px 0px;
  box-sizing: border-box;
  border-radius: 5px;
  overflow: hidden;
  .scroll-j-tr {
	display: table-row;
	//height: 100%;
	&:first-of-type {
	  //height: 1%;
	  .scroll-j-td {
		height: auto;
	  }
	}
	.scroll-j-td {
	  display: table-cell;
	  width: 100%;
	  height: 100%;
	  vertical-align: top;
	  position: relative;
	}
  }
}
.scroll-j-scroll {
  position: absolute;
  height: 100%;
  @include scroll($scroll: auto);
  border-bottom-left-radius: 5px;
  width: 100%;
  top: 0;
  left: 0;
  right: 0px;
  bottom: 0px;
}
.scroll-j-scroll-wrap {
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
}
.centr-market-contain .scroll-contain.multiview-wrapper {
  background: none;
  .team-name-market, .team-name-market.info, .hide-on-mv-j {
    display: none;
  }
}
.multiview-contain, .dndPlaceholder{
  float: left;
  width: calc(50% - 6px);
  margin: 0 5px 5px 0;
  height: 400px;
  background: #161B24;
  box-sizing: border-box;
  border-radius: 5px;
  transition: 250ms background;
  list-style: none;
  .scroll-j-td {
    background: #161B24;
  }
  .scroll-j-scroll {
  scrollbar-track-color: #161B24;
  scrollbar-shadow-color: #161B24;
  }
  .scroll-j-scroll::-webkit-scrollbar {
    background: #161B24;
  }
  .game-contain-j .scroll-j-tr:first-of-type .scroll-j-td {
    border-radius: 6px 6px 0 0;
  }
  .game-contain-j .scroll-j-tr:last-of-type .scroll-j-td {
    border-radius: 0 0 4px 4px;
  }
  @media screen and (max-width: 1333px) {width: 100%;}
  .game-contain-j {
    padding: 5px;
  }
  .game-score-multiset ul.title .mini-icon-1 {
    margin: 8px 0 0 0;
  }
  .live-game-container {
	.game-info .game-detailed-stats {
      top: 81px;
	}
    .game-content {
      border-radius: 5px;
      .game-info {
        border-radius: 8px 8px 0 0;
        box-shadow: none;
        margin-bottom: 0px;
		&.game-tennis {
		  margin-bottom: 10px;
		}
      }
      .unpin-button {
        display: none;
      }
    }
  }
  .mini-market-b:last-of-type {
    margin-bottom: 0px;
  }
  .game-score-multiset ul.title, .live-game-container .sport-title {
    height: 36px;
	background: #212532;
	border: 0px;
	transition-duration: 200ms;
	line-height: 36px;
    overflow: hidden;
    border-radius: 5px 5px 0 0;
    font-size: 12px;
    font-weight: normal;
    li {
      vertical-align: middle;
      background: #212532;
      border: 0px;
      transition-duration: 200ms;
      font-size: 12px;
      text-shadow: none;
      &:first-child {
        border-top-left-radius: 5px;
      }
      &:last-child {
        border-top-right-radius: 5px;
      }
    }
  }
  .close-multiv-j {
    display: block;
    height: 36px;
    width: 30px;
    background: #212532;
    cursor: pointer;
    position: absolute;
    top: 0px;
    right: 0px;
    z-index: 10;
    border-top-right-radius: 5px;
    text-align: center;
    font: 12px/36px icomoon;
    color: #7b8191;
    transition-duration: 200ms;
    &:hover {
      color: white;
    }
    &:before {
      content: "\e117";
    }
  }
  .game-info .game-horse-racing ul.race-title {
	height: 36px;
	box-shadow: none;
	background: #212532;
  }
  &.active {
    background-color: #262A37;
    .game-score-multiset ul.title li, .close-multiv-j, .live-game-container .sport-title, .game-info .game-horse-racing ul.race-title{
      box-shadow: inset 0px -2px 0px $red-bg;
      background-color: #2E323E;
    }
  }
  .game-score-multiset ul li.right-arrow:before, .game-score-multiset ul li.left-arrow:before {
    top: 7px;
  }
  .game-score-multiset ul:nth-child(2) li:first-child p, .game-score-multiset ul:nth-child(3) li:first-child p {
    margin-left: 10px;
  }
  .game-score-multiset ul li,  {
