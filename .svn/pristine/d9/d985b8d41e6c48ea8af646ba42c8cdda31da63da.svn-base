.statistics-toggle .game-list-statistics{
    font-size: 12px;
    width: 100%;
    float: left;
}
.statistics-toggle .game-list-statistics td{
    width: 30px;
    border-top: 1px solid #535A3C;
    border-right: 1px solid #535A3C;
    /*padding: 4px 0 1px;*/
	font-size: 12px;
    text-align: center;
    vertical-align: middle;
}
.statistics-toggle .game-list-statistics td:first-child{
    width: 100%;
    text-align:left;
}
.statistics-toggle .game-list-statistics td:last-child{border-right: 0;}
.statistics-toggle .game-list-statistics th span{
    display: block;
    width:35px;
    height: 10px;
    position: relative;
    cursor: default;
}
.statistics-toggle .game-list-statistics th span.y-card:after,.statistics-toggle .game-list-statistics th span.r-card:after {
    content: "";
    width: 8px;
    height: 10px;
    background: #FC0;
    display: block;
    position: absolute;
    box-shadow: 1px 1px 0 #000;
    left: 13px;
}
.statistics-toggle .game-list-statistics th span.r-card:after {
    background: #f00;
}
.statistics-toggle .game-list-statistics th .corner span {
    position: absolute;
    overflow: hidden;
    width: 9px;
    height: 9px;
    left: 13px;
    border-bottom: 1px solid #fff;
    border-left: 1px solid #fff;
    box-shadow: -1px 1px 0 #000;
    z-index: 5;
}
.statistics-toggle .game-list-statistics th .corner span:after {
    content: "";
    top: 4px;
    left: -6px;
    width: 9px;
    height: 9px;
    border-radius: 50%;
    position: absolute;
    border: 1px solid #fff;
    box-shadow: inset 0px 1px 0 #000;
}
.statistics-toggle .game-list-statistics th .penalty span {
    position: absolute;
    left: 10px;
    width: 14px;
    height: 9px;
    background: #35800F;
    border: 1px solid #fff;
}
.statistics-toggle .game-list-statistics th .penalty span:before {
    content: "";
    position: absolute;
    width: 8px;
    height: 3px;
    top: -1px;
    left: 2px;
    border: 1px solid #fff;
    border-color: transparent #fff #fff #fff;
}
.statistics-toggle .game-list-statistics th .penalty span:after {
    content: "";
    position: absolute;
    width: 4px;
    height: 4px;
    top: 4px;
    left: 5px;
    border-radius: 50%;
    background: #f00;
}
.statistics-toggle .game-list-statistics td:first-child span{
    float: left;
    margin: 0 10px;
}
.statistics-toggle .game-list-statistics td:first-child p{
    margin-top: 3px;
    float: left;
    width: 480px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.statistics-toggle.flip-contain{
  width: 100%;
  height: 62px;
  position: relative;
  perspective: 800px;
  -webkit-perspective: 800px;
}
.statistics-toggle.flip-contain .flip-card{
    width: 100%;
    height: 100%;
    position: absolute;
}
.body-wrapper:not(.multiview) {
  .statistics-toggle.flip-contain .flip-card.flipped-0 .timeline-container, .statistics-toggle.flip-contain .flip-card.flipped-0 .back,
  .statistics-toggle.flip-contain .flip-card.flipped-1 .front, .statistics-toggle.flip-contain .flip-card.flipped-1 .timeline-container,
  .statistics-toggle.flip-contain .flip-card.flipped-2 .front, .statistics-toggle.flip-contain .flip-card.flipped-2 .back{
    -webkit-transform: rotateX(90deg);
    transform: rotateX(90deg);
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
    opacity: 0;
  }
  .statistics-toggle.flip-contain .flip-card.flipped-1 .back{
    -webkit-transform: rotateX(0deg);
    transform: rotateX(0deg);
    opacity: 1;
  }
}
.statistics-toggle.flip-contain .flip-card .timeline-container{
    display: block;
    position: absolute;
    -webkit-transform-origin: 50% 0;
    transform-origin: 50% 0;
    transition-duration: 400ms;
}
.statistics-toggle.flip-contain .flip-card .front, .statistics-toggle.flip-contain .flip-card .back{
    display: block;
    position: absolute;
    width: 100%;
    height: 100%;
    -webkit-transform-origin: 50% 0;
    transform-origin: 50% 0;
    transition-duration: 400ms;
}
.statistics-toggle.flip-contain .flip-card .back{
    -webkit-transform: rotateX(-90deg);
    transform: rotateX(-90deg);
    -webkit-transform-origin: 50% 0;
    transform-origin: 50% 0;
    opacity: 0;
}
.statistics-toggle.flip-contain .flip-btn{
    position: absolute;
    left: 50%;
    bottom: 6px;
    width: 28px;
    height: 10px;
    margin-left: -22px;
    padding: 2px;
    z-index: 9999;
    background: #ccc;
    opacity: 0.8;
    border-radius: 3px 3px 0 0;
    text-shadow: none;
