@import 'classic-colors';
@font-face {
  font-family: 'icomoon';
  src:url('../../../fonts/icomoon.eot?oladcq0');
  src:url('../../../fonts/icomoon.eot?#iefixolacdq0') format('embedded-opentype'),
  url('../../../fonts/icomoon.woff?oladcq0') format('woff'),
  url('../../../fonts/icomoon.ttf?olsdacq0') format('truetype'),
  url('../../../fonts/icomoon.svg?olsdacq0#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: "ucraftme-v1";
  src:url("../../../fonts/ucraftme-v1.eot");
  src:url("../../../fonts/ucraftme-v1.eot?#iefix") format("embedded-opentype"),
  url("../../../fonts/ucraftme-v1.woff") format("woff"),
  url("../../../fonts/ucraftme-v1.ttf") format("truetype"),
  url("../../../fonts/ucraftme-v1.svg#ucraftme-v1") format("svg");
  font-weight: normal;
  font-style: normal;
}
@mixin scroll($scroll: scroll, $width: 4px, $background: $scroll-main-color) {
  @if $background == light {
    $background: #7b8191;
    $scroll-main-thumb-color: #505767;
  }
  overflow-y: $scroll;
  overflow-x: hidden;
  scrollbar-base-color: $background;
  scrollbar-track-color: $background;
  scrollbar-shadow-color: $background;
  scrollbar-base-color: $scroll-main-thumb-color;
  scrollbar-arrow-color: $scroll-main-thumb-color;
  &::-webkit-scrollbar-track {
	border-radius: 5px;
  }
  &::-webkit-scrollbar {
	width: $width;
	background: $background;
	border-radius: 5px;
  }
  &::-webkit-scrollbar-thumb {
	border-radius: 10px;
	background: $scroll-main-thumb-color;
	transition-duration: 100ms;
	&:hover {
	  background: lighten( desaturate( $scroll-main-thumb-color, 1 ), 6 );
	}
  }
}
[data-icon]:before {
  font-family: "ucraftme-v1" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
[class^="icon-"], [class*=" icon-"] {
  font-family: 'icomoon';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.ellipsis-text{
	overflow:hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
