.calendar-nav{
  float: left;
  width: 100%;
  position: relative;
  background: #373738;
  border-top: 1px solid #484848;
  border-bottom: 1px solid #222;
}
.calendar-nav label{
  float: left;
  width: calc(100% - 70px);
  color: $white-text;
  font: 12px/16px $font-f;
  text-transform: uppercase;
  display: block;
  padding: 5px 20px 5px 50px;
  text-overflow: ellipsis;
  white-space: nowrap;
  z-index: 33;
  font-weight: bold;
  cursor: pointer;
}
.calendar-nav label span.mini-icon-1{
  height: 16px;
  position: absolute;
  left: 8px;
  top: 3px;
}
.calendar-nav label span.mini-icon-1:before{display: none;}
.calendar-nav input[type="checkbox"]{
  position: absolute;
  left: -9999em;
}
.calendar-nav input[type="checkbox"] + label:before{left: 28px;top: 6px;}
.calendar-nav input[type="checkbox"]:checked + label:after{left: 28px;top: 5px;}
.calendar-nav.active:before, .calendar-nav:hover:before{
  content: "";
  display: block;
  position: absolute;
  left: 0;
  height: 100%;
  width: 3px;
  background: #a61f67;
  left: 0;
  top: 0;
}
.left-list-menu ul li .calendar-nav-b, .left-menu-contain.mini .left-list-menu ul li .calendar-nav-b, .left-menu-contain.mini .left-list-menu ul li.active .calendar-nav-b{
