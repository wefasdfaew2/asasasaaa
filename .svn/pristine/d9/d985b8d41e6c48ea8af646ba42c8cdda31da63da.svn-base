  .statistics-toggle.flip-contain .flip-card.flipped-multivew-2 .front, .statistics-toggle.flip-contain .flip-card.flipped-multivew-2 .back{
	-webkit-transform: rotateX(90deg);
	transform: rotateX(90deg);
	-webkit-transform-origin: 50% 100%;
	transform-origin: 50% 100%;
	opacity: 0;
  }
  .statistics-toggle.flip-contain .flip-card.flipped-multivew-1 .back{
	-webkit-transform: rotateX(0deg);
	transform: rotateX(0deg);
	opacity: 1;
  }
  .game-score-multiset ul.title li:last-child {
	min-width: 20px;
  }
  .mini-market-right {
	padding-bottom: 5px;
  }
